/**
 * Sample business network definition.
 */
namespace org.example.basic

asset Vehicle identified by vehicleId {
  o String vehicleId
  --> VehicleOwner owner
  o String brand
  o String model
  o Integer year
}

participant VehicleOwner identified by personId {
  o String personId
  o String firstName
  o String lastName
  o Integer birthYear
}

transaction VehicleTransfer {
  --> Vehicle asset
  o String newOwner
}

event VehicleTransferEvent {
  --> Vehicle asset
  o String oldOwner
  o String newOwner
}
